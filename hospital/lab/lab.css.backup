body {
    font-family:sans-serif;
    margin: 0;
}
#page {
    position: relative;
}
caption {
    line-height:25px;
    border-style: solid;
    border-color: black;
    border-width: 1px 1px 0;
}
#nhi {
    font-weight: bold;
}
#toplink {
    background-color: yellow;
}
#left {
    width: 190px;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    padding: 0 5px;
    background-color: darkgreen;
}
#sidebar {
    list-style: none;
    margin: 0;
    padding: 0;
}
.side {
    margin: 5px 0;
    margin-left: 0;
}
.side, #toplink {
    text-align:left;
    display: block;
    text-decoration: none;
    line-height:25px;
}
#toplink>span, .side {
    padding-left: 20px;
}
a, a:active, a:focus, a:visited {
    color: black;
}
a:hover, #toplink:hover {
    background-color: black;
    color: white;
}
#right {
    margin-left: 200px;
    padding: 5px;
}
table {
    border-collapse: collapse;
    border-style: solid;
    border-color: black;
    border-width: 0 1px 1px 1px;
    margin-bottom: 5px;
}
td,th {
    padding: 2px;
    min-width: 80px;
    text-align: center;
}
thead {
    vertical-align: bottom;
}
em {
    font-style: normal;
    color: red;
    font-weight: 700;
}
.p1 {
    background-color: #ff6666;
}
.p2 {
    background-color: #b5ffb5;
}
.p3 {
    background-color: #7d7dff;
}
.p4 {
    background-color: #d6d6d6;
}
.p5 {
    background-color: #ff7f00;
}
.p6 {
    background-color: #ff9efd;
}
tbody>tr:nth-child(odd) {
    background-color: #dae5f4;
}
